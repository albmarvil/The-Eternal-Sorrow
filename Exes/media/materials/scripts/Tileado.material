material Tiles/Castillo/Frontal : TilesPadreCastillo
{
	technique 0
	{
		pass 1
		{
			//Textura del tile
			texture_unit
			{
				texture Castillo_Frontal_TX.png //luz difusa
				tex_coord_set 0

				filtering anisotropic
				max_anisotropy 16
			}

			//Textura del bump map
			texture_unit
			{
				texture Castillo_Frontal_Normal.png //Normal map a la que aplicar la luz difusa
				tex_coord_set 1

				filtering anisotropic
				max_anisotropy 16
			}

			//Textura del tile
			texture_unit
			{
				texture Castillo_Frontal_TX.png //sumar color
				tex_coord_set 2

				filtering anisotropic
				max_anisotropy 16
			}
		}
	}
}

