				param_named_auto lightPos0 light_position 0
                param_named_auto lightAtt0 light_attenuation 0
                param_named_auto tiempo sintime_0_2pi 20
                param_named brillo float 1.5
			}
		}
	}

	technique 1
	{
		pass 1
		{
			texture_unit
			{
				texture textError.jpg
			}
		}
	}
}

